<h1>user#index</h1>
     <table class="table">
       <thead>
         <tr>
           <th>Title</th>
           <th>Opinion</th>
         </tr>
       </thead>
       <tbody>
         <% @users.each do |user| %>
         <tr>
           <td><%= user.id %></td>
           <td><%= user.name %></td>
           <td><%= user.email %></td>
         </tr>
       </tbody>
         <% end %>
      </table>

       <table class="table">
       <thead>
         <tr>
           <th>Title</th>
           <th>Opinion</th>
         </tr>
       </thead>
       <tbody>
         <% @books.each do |book| %>
         <tr>
           <td><%= book.user_id %></td>
           <td><%= book.title %></td>
           <td><%= book.body %></td>
         </tr>
       </tbody>
         <% end %>
      </table>