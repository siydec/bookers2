<div class="container">
  <p></p>
  <div class="row">
    <%= render partial: 'books/leftside', locals: {book: @book} %>
    <div class="col-md-8 offset-md-1">
     <h2>Users</h2>
     <table class="table">
       <thead>
         <tr>
           <th>image</th>
           <th>name</th>
         </tr>
       </thead>
       <tbody>
         <% @users.each do |user| %>
         <tr>
           <td><%= image_tag user.get_profile_image("40x40") %></td>
           <td><%= user.name %></td>
           <td><%= link_to "Show", controller:"users", action: "show", id: user %></td>
         </tr>
       </tbody>
         <% end %>
      </table>
    </div>
  </div>
</div>